{
  "name": "gitlab helper",
  "description": "help using gitlab",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "tabs",
    "declarativeContent",
    "scripting",
    "activeTab"
  ],
  "action": {
    "default_title": "Hello",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+B",
        "mac": "MacCtrl+B"
      }
    }
  }
}
